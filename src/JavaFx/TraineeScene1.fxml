         <font>
            <Font size="17.0" />
         </font>
      </Button>
      <Button fx:id="deleteButton" disable="true" layoutX="571.0" layoutY="538.0" mnemonicParsing="false" onAction="#deleteTrainee" prefHeight="38.0" prefWidth="89.0" text="Delete">
