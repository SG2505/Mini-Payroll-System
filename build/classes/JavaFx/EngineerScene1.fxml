<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="scenePane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.EngineerScene1Controller">
   <children>
      <TableView fx:id="tableView" layoutX="73.0" layoutY="14.0" onMouseClicked="#getSelected" prefHeight="528.0" prefWidth="653.0">
        <columns>
          <TableColumn fx:id="Name" prefWidth="75.0" text="Name" />
          <TableColumn fx:id="ID" prefWidth="75.0" text="ID" />
            <TableColumn fx:id="Age" prefWidth="75.0" text="Age" />
            <TableColumn fx:id="Position" prefWidth="75.0" text="Position" />
            <TableColumn fx:id="WorkingHours" prefWidth="75.0" text="Working Hours" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="exitButton" layoutX="29.0" layoutY="554.0" mnemonicParsing="false" onAction="#exit" text="Exit">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="backButton" layoutX="715.0" layoutY="554.0" mnemonicParsing="false" onAction="#back" prefHeight="37.0" prefWidth="66.0" text="Back">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="addButton" layoutX="132.0" layoutY="551.0" mnemonicParsing="false" onAction="#addEngineer" prefHeight="38.0" prefWidth="79.0" text="Add">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="editButton" disable="true" layoutX="245.0" layoutY="550.0" mnemonicParsing="false" onAction="#edit" prefHeight="38.0" prefWidth="79.0" text="Edit">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="deleteButton" disable="true" layoutX="357.0" layoutY="550.0" mnemonicParsing="false" onAction="#delete" prefHeight="38.0" prefWidth="79.0" text="Delete">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="calacSalaryButton" disable="true" layoutX="475.0" layoutY="550.0" mnemonicParsing="false" onAction="#calculate" prefHeight="38.0" prefWidth="196.0" text="Calculate Salary">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
